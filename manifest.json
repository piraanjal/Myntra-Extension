{
    "manifest_version": 3,
    "name": "Page URL Recorder",
    "version": "1.0",
    "description": "Stores and displays the current page URL.",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "./assests/logo.png"
    }
  }
  